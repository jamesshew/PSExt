      <AdditionalUsingDirectories>$(MSBuildProjectDirectory)\..\PSExtCmdlets\bin\$(Configuration)</AdditionalUsingDirectories>
    </ClCompile>
    <Link>
      <AdditionalDependencies>shlwapi.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>